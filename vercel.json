{
    "builds": [
        {
            "src": "/app/main.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "{agent_adapter/**,a2a_samples/**,autogen_demo/**,**/*.test.py,**/test_*.py,google_colab/**,oauth_client/**,public_opinion_monitoring/**,sample-data/**,static/**,assets/**}"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.)",
            "dest": "app/main.py"
        }
    ]
}